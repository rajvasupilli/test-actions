{
    "family": "sbt-task-definition",
    "executionRoleArn": "arn:aws:iam::238029455348:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "sbt-container",
            "image": "238029455348.dkr.ecr.us-east-1.amazonaws.com/sbtrepo:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 9000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}
