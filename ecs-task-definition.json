{
    "family": "ec2-task-definition",
    "executionRoleArn": "arn:aws:iam::724950795606:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "jenkins",
            "image": "724950795606.dkr.ecr.us-east-1.amazonaws.com/actions:latest",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
