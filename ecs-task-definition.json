{
    "family": "ecs-task-definition",
    "executionRoleArn": "arn:aws:iam::460087368253:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "ecs-container",
            "image": "460087368253.dkr.ecr.us-east-1.amazonaws.com/ecs-repo",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
           "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "awslogs-ecsservice",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "awslogs-ecs"
                }
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}
